{
  "type": "modal",
  "styles": "ModalStyle",
  "class": "modal-standard",
  "elements": [
    {
      "type": "header",
      "class": "header",
      "elements": [
        {
          "type": "h2",
          "class": "heading",
          "text": "Import List"
        },
        {
          "type": "basic icon button",
          "class": "close-button",
          "text": "close"
        }
      ]
    },
    {
      "type": "content",
      "class": "form-content-padded",
      "elements": [
        {
          "type": "ImportFileForm",
          "notes": "Transitions to ImportFileTable after the file upload"
        }
      ]
    },
    {
      "type": "footer",
      "class": "footer",
      "elements": [
        {
          "type": "flat basic button",
          "text": "Cancel",
          "notes": "Closes modal on click"
        },
        {
          "type": "flat primary button",
          "text": "Import",
          "notes": "Enabled when at least one Compatible or Warning file is uploaded. Closes modal on click. Adds row(s) to FeatureFlagIncludeListsTable or FeatureFlagExcludeListsTable"
        }
      ]
    }
  ]
}