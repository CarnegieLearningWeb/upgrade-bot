{
  "type": "modal",
  "styles": "ModalStyle",
  "class": "modal-standard",
  "elements": [
    {
      "type": "header",
      "class": "header",
      "elements": [
        {
          "type": "h2",
          "class": "heading",
          "text": "Add Exclude List"
        },
        {
          "type": "basic icon button",
          "class": "close-button",
          "text": "close"
        }
      ]
    },
    {
      "type": "content",
      "class": "form-content-standard",
      "elements": [
        {
          "type": "FeatureFlagExcludeListForm",
          "notes": "First form field should be focused"
        }
      ]
    },
    {
      "type": "footer",
      "class": "footer",
      "elements": [
        {
          "type": "flat basic button",
          "text": "Cancel",
          "notes": "Closes modal on click"
        },
        {
          "type": "flat primary button",
          "text": "Create",
          "notes": "Enabled when the form is valid. Closes modal on click. Adds a row to FeatureFlagExcludeListsTable"
